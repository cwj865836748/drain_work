/* components/navbar/index.wxss */
.navbar {
  width: 100%;
  position: relative;
  z-index: 3;
  transition: all 0.1s;
}
.navbarFixed {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 3;
}
.title-container {
  display: flex;
  align-items: center;
  position: relative;
}
.title_head {
  position: absolute; 
  transform: translate(-50%, -50%); 
  left: 50%; 
  top: 50%; 
  font-size: 17px; 
  font-weight: bold;
  max-width: 400rpx;
}
.slotContent {
  overflow: auto;
}
.slotContent2 {
  min-height:100%;
  /* height: 100%; */
}
.tabbarTitle {
  font-size: 22rpx;
  color: #9193a0;
}
.tabbarActive {
  color: #171a2d;
  font-weight: bold;
}
.slotPadding {
  padding-bottom: 50px ;
}
.slotIphoneXPadding {
  padding-bottom: 84px ;
}
.slotIphoneXPadding2 {
  padding-bottom: 34px ;
}
