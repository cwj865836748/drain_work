/* pages/inspectionProcess/inspectionProcess.wxss */
.processHead {
  height: 276rpx;
  background: #ffffff;
  padding: 32rpx 30rpx 0;
}

.processOne:first-child {
  margin-bottom: 34rpx;
}

.processTitle {
  color: var(--c2);
}

.processAddress {
  font-size: 32rpx;
  color: var(--c1);
  font-weight: bold;
  margin-top: 8rpx;
}

.processTime {
  display: inline-block;
  width: 72rpx;
  height: 82rpx;
  background: #f6f6f6;
  border-radius: 14rpx;
  font-size: 44rpx;
  font-family: DINAlternate, DINAlternate-Bold;
  color: #008ffe;
  margin-right: 30rpx;
  text-align: center;
  line-height: 82rpx;
  position: relative;
  /* padding: 16rpx 15rpx; */
  font-weight: bold;
}

.processTime:last-child {
  margin-right: 0rpx;
}

.processTime:nth-child(-n+2)::after {
  content: ":";
  position: absolute;
  right: -20rpx;
  top: 50%;
  transform: translateY(-50%);
  color: #008ffe;
  font-size: 36rpx;
}

.processCard {
  padding: 40rpx 30rpx 192rpx
}

.processCardTitle {
  font-size: 30rpx;
  color: var(--c1);
  margin-bottom: 24rpx;
}

.processCardTitle::before {
  content: "";
  display: block;
  width: 8rpx;
  height: 34rpx;
  background: #008ffe;
  border-radius: 2rpx;
  margin-right: 6rpx;
}

.processCardOne {
  width: 334rpx;
  height: 164rpx;
  background: #ffffff;
  border-radius: 12rpx;
  margin-bottom: 22rpx;
  padding: 32rpx 0 0 30rpx;
  color: var(--c2);
}

.processCardOne:nth-child(odd) {
  margin-right: 22rpx;
}

.processCardOne:nth-last-child(-n+2) {
  margin-bottom: 0rpx;
}

.processCardOneHead {
  font-size: 24rpx;
  margin-bottom: 18rpx;
}

.processCardOneHead>image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 4rpx;
}

.processCardOneTime {
  font-size: 36rpx;
  letter-spacing: 1rpx;
}
.timeShow {
  color:var(--c1);
  letter-spacing: 0rpx;
  font-weight: bold;
}
map {
  width: 100%;
  height: 200px;
  border-radius: 8rpx;
  overflow: hidden;
  margin: 28rpx 0 42rpx;
}

.cardWarnTitle {
  color: var(--c2);
  margin-bottom: 16rpx;
}

.cardContent {
  font-size: 26rpx;
  color: var(--c1);
  line-height: 44rpx;
}
.cardContent:first-child {
  margin-bottom: 12rpx;
}
.cardContent>text {
  color:#008FFE;
  text-decoration:underline;
}
.safeBottom {
  padding-bottom: 260rpx;
}
.dialogOk {
  height: 388rpx;
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
  padding-top: 40rpx;
}
.dialogOk>.fail {
  color: #ea3652;
}
.dialogOk>image {
  width: 84rpx;
  height: 84rpx;
  margin-bottom: 16rpx;
}
.dialogBtn {
  width: 100%;
  height: 100rpx;
  border-top: 2rpx solid #efefef;
  color: #008ffe;
  margin-top: 36rpx;
}
.van-button {
  width: 100% !important;
  text-align: center;
  font-size: 32rpx !important;
  color: #008ffe !important;
}
.dialogDetail {
  margin-top: 28rpx;
  font-size: 26rpx;
  color:var(--c1)
}
.dialogDetailOne:first-child {
  margin-right: 36rpx;
}
.dialogDetailOne>image {
 width: 36rpx;
 height: 36rpx;
 margin-right: 6rpx;
}
.dialogFail {
  color:var(--c1);
  font-weight: normal;
  margin-top: 24rpx;
  font-size: 28rpx;
}
.van-popup {
  border-radius: 20rpx!important;
}
.beTimeout {
  color:#EA3652;
  font-size: 34rpx;
}